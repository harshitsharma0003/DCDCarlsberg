pipeline {
    agent any
    stages
    {
       
        stage('Git Clone')
        {
            steps	{
		    sh 'rm -rf ;  git clone https://github.com/harshitsharma0003/DCDCarlsberg.git' 
		    }
        }
        stage('Git clone done - just telling')
        {
            steps	{
		       sh 'echo -en "Done git clone..."' 
		    }
        }
     }
}
        

